
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="row">
    <div class="col-10">
        <h2 class="text-primary">Danh sách nhà bán hàng </h2>
    </div>
    <div class="col-2 text-right">
        <a class="btn btn-primary" asp-action="Upsert"><i class="fas fa-plus"></i> &nbsp; Tạo Mới</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered pt-2" style="width: 100%">
        <thead class="table table-dark">
            <tr>
                <th>Tên</th>
                <th>Email</th>
                <th>SĐT</th>
                <th>Số nhà, đường</th>
                <th>Xã</th>
                <th>Huyện</th>
                <th>Thành Phố</th>
                <th>Là nhà bán hàng</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/company.js"></script>
}