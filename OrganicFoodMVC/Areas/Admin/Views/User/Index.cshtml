
@{
    ViewData["Title"] = "Danh sách người dùng";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<br />
<div class="row">
    <div class="col-10">
        <h2 class="text-primary">Danh sách người dùng </h2>
    </div>
    <div class="col-2 text-right">
        <a class="btn btn-primary" asp-area="Identity" asp-page="/Account/Register"><i class="fas fa-plus"></i> &nbsp; Tạo Mới</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered pt-2" style="width: 100%">
        <thead class="table-dark">
            <tr class="table-info">
                <th>Tên</th>
                <th>Email</th>
                <th>SĐT</th>
                <th>Nhà bán hàng</th>
                <th>Quyền</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/user.js"></script>
}